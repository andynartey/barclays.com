.site-container {
    background: #fff;
    position: relative;
    padding: 0;
}

.content {
    position: relative;
}

.main-content {
    padding-top: 20px;
    // min-height: 505px;
    position: relative;
    @include clearfix;
}


.secondary-content {
    @include clearfix;
}

.orbit-timer {
    display: none;
}


@media #{$medium} {
    .secondary-content {
        margin-left: 0;
        float: right;
    }
}


.tertiary-content {
    @include clearfix;
}


@media #{$small} {
    .offset-left-medium-1 {
        margin-left: -8.33333%;
    }
}

@media #{$medium} {
    .offset-left-medium-1 {
        left: 0;
    }
}