#CQ label {
  font-size: 12px;
  color: #000;
  cursor: pointer;
  display: block;
  font-weight: 500;
  margin-bottom: 0;
}

#CQ form {
  margin: 0;
}

#CQ *,
#CQ :after,
#CQ :before {
  -moz-box-sizing: content-box;
  -web-box-sizing: content-box;
  box-sizing: content-box;
}