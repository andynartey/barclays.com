@import url('app-ie-blessed1.css');

.disclaimer {
  position: relative;
  background: #FFF;
  padding: 1.25em;
  width: auto;
  max-width: 42.5em;
  margin: 1.25em auto;
  border: 5px solid #e1e1e1;
  border-radius: 5px;
  -webkit-box-shadow: 0 0 12px 4px #cfcfcf;
  box-shadow: 0 0 12px 4px #cfcfcf;
}

.disclaimer {
  border: 10px solid #e1e1e1;
  border-radius: 10px;
}

.disclaimer__inner {
  max-height: 12.5em;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  padding-bottom: 1.25em;
  background: -moz-linear-gradient(top,rgba(0,0,0,0) 1%,rgba(237,237,237,0) 97%,rgba(244,244,244,.65) 100%);
  background: -webkit-gradient(linear,left top,left bottom,color-stop(1%,rgba(0,0,0,0)),color-stop(97%,rgba(237,237,237,0)),color-stop(100%,rgba(244,244,244,.65)));
  background: -webkit-linear-gradient(top,rgba(0,0,0,0) 1%,rgba(237,237,237,0) 97%,rgba(244,244,244,.65) 100%);
  background: -o-linear-gradient(top,rgba(0,0,0,0) 1%,rgba(237,237,237,0) 97%,rgba(244,244,244,.65) 100%);
  background: -ms-linear-gradient(top,rgba(0,0,0,0) 1%,rgba(237,237,237,0) 97%,rgba(244,244,244,.65) 100%);
  background: linear,to bottom,rgba(0,0,0,0) 1%,rgba(237,237,237,0) 97%,rgba(244,244,244,.65) 100%;
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#00000000', endColorstr='#a6f4f4f4', GradientType=0);
}

.disclaimer__inner {
  max-height: 21.25em;
}

.disclaimer__btns {
  padding-top: 1.25em;
}

.disclaimer__btns .btn {
  text-align: center;
  font-size: 1.125em;
}

.disclaimer__btns .btn {
  text-align: left;
}

.disclaimer__btns .btn__primary {
  margin-right: .9375em;
}

.re-direct .disclaimer__inner {
  overflow-y: visible;
}

.topic-results__count {
  margin-top: 0!important;
  padding-bottom: 2.5em;
}

.topic-results__filter {
  border-top: 1px solid #e1e1e1;
  border-bottom: 1px solid #e1e1e1;
  margin-bottom: 1.875em;
}

.topic-results__filter__col:last-child {
  display: block;
}

.topic-results__filter__col:last-child {
  float: left;
}

.topic-results__results {
  padding-bottom: 2.5em;
}

.topic-results__item__inner {
  margin-left: auto;
  margin-right: auto;
  margin-top: 0;
  margin-bottom: 0;
  max-width: 75em;
  width: 100%;
  *zoom: 1;
}

.topic-results__item__inner:after,
.topic-results__item__inner:before {
  content: " ";
  display: table;
}

.topic-results__item__inner:after {
  clear: both;
}

.topic-results__item__copy.topic-border {
  border-top: 4px solid #e1e1e1;
}

.topic-results__similar {
  border-top: 1px solid #e1e1e1;
  padding: 1.25em 0 1.875em;
}

.topic-results__similar h2 {
  margin-top: 0!important;
  font-size: 1.5em;
  color: #00395d;
}

.topic-results__search-field {
  *zoom: 1;
  position: relative;
}

.topic-results__search-field:after,
.topic-results__search-field:before {
  content: " ";
  display: table;
}

.topic-results__search-field:after {
  clear: both;
}

.topic-results__search-field .topic-results__input {
  width: 88%;
  float: left;
  -webkit-box-shadow: none;
  box-shadow: none;
  border-radius: 3px 0 0 3px;
  border: 1px solid #d9d9d9;
  border-right: 0;
  height: 44px;
  font-size: 1.5em;
  font-family: Expert-Sans-Light,Helvetica,Helvetica,Arial,sans-serif;
  color: #8c8c8c;
  padding-top: 0;
  padding-bottom: 0;
}

.topic-results__search-field .topic-results__input:focus {
  border-color: #d9d9d9;
  box-shadow: none;
  outline: 0;
  background: 0 0;
}

.topic-results__search-field .topic-results__input {
  width: 95%;
}

.topic-results__search-field .topic-results__submit {
  width: 5%;
  float: left;
  background: 0 0;
  border-radius: 0 3px 3px 0;
  border: 1px solid #d9d9d9;
  border-left: 0;
  height: 44px;
  position: relative;
  z-index: 10;
}

.topic-results__search-field .icon__search {
  position: absolute;
  top: .25em;
  right: .5em;
  z-index: 5;
}

.topic-results__search-field .icon__search:after {
  color: #8c8c8c;
  font-size: 1.375em;
}

.share-price-feed {
  background: #f2f8fb;
  border-top: 6px solid #c2e0ee;
  padding: 1.5625em .9375em;
  min-height: 20.625em;
  position: relative;
  margin-bottom: .9375em;
}

.share-price-feed {
  margin-bottom: 0;
}

.share-price-feed h3 {
  font-family: Expert-Sans-Regular,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 1.25em;
  color: #00395d;
  margin-bottom: 1.25em;
  line-height: 1.2;
}

.share-price-feed h3 {
  font-size: 1.375em;
  line-height: 1.181818;
}

.share-price-feed__price {
  color: #007eb6;
  margin-bottom: .9375em;
}

.share-price-feed__price .value {
  font-family: Expert-Sans-Light,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 2.75em;
  line-height: 1.18;
  margin-right: .3125em;
}

.share-price-feed__price .value {
  font-size: 3.6875em;
  line-height: 1;
}

.share-price-feed__price .change {
  font-family: Expert-Sans-Light,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 1.25em;
}

.share-price-feed__price .change:before {
  font-size: .875em;
  margin-right: .625em;
  position: relative;
  top: 0;
}

.share-price-feed__price .change.up:before {
  font-family: Icons;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e622";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.share-price-feed__price .change.down:before {
  font-family: Icons;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e601";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.share-price-feed__price .change {
  font-size: 1.6875em;
}

.share-price-feed__date {
  font-family: Expert-Sans-Regular,Helvetica,Helvetica,Arial,sans-serif;
  color: #007eb6;
  font-size: .875em;
}

.share-price-feed__info {
  font-family: Expert-Sans-Regular,Helvetica,Helvetica,Arial,sans-serif;
  font-size: .625em;
  color: #8c8c8c;
  margin-bottom: 2.8125em;
}

.share-price-feed__links {
  position: absolute;
  bottom: .625em;
}

.share-price-feed__link {
  display: block;
  font-family: Expert-Sans-Light,Helvetica,Helvetica,Arial,sans-serif;
  font-size: .875em;
  line-height: 2.1428;
}

.share-price-feed__link {
  font-size: 1em;
  line-height: 1.875;
}

.share-price-feed__link:after {
  position: relative;
  top: .0625em;
  font-family: Icons;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e616";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-size: .875em;
  margin-left: .25em;
}

.share-price-feed__link.external:after {
  font-family: Icons;
  speak: none;
  font-style: normal;
  font-weight: 400;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  content: "\e617";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.tabs-basic.no-tabs-mode .tabs-basic__menu {
  border: 0;
}

.tabs-basic.no-tabs-mode .tabs-basic__menu a {
  font-family: Expert-Sans-Regular,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 1.5em;
  color: #00395d;
  margin-bottom: .625em;
}

.tabs-basic.no-tabs-mode .tabs-basic__menu a {
  font-size: 1.6875em;
  line-height: 1.21;
}

.tabs-basic.no-tabs-mode .tabs-basic__menu a:hover {
  cursor: default;
}

.tabs-basic__menu {
  list-style: none!important;
  margin: 0 0 1.25em;
  border-bottom: 1px solid #c2e0ee;
  *zoom: 1;
}

.tabs-basic__menu:after,
.tabs-basic__menu:before {
  content: " ";
  display: table;
}

.tabs-basic__menu:after {
  clear: both;
}

.tabs-basic__menu li {
  float: left;
  margin-right: 1.875em;
}

.tabs-basic__menu li {
  margin-right: .9375em;
}

.tabs-basic__menu li {
  margin-right: 1.25em;
}

.tabs-basic__menu a {
  font-family: Expert-Sans-Regular,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 1.25em;
  line-height: 1.375;
  display: inline-block;
  padding-bottom: .25em;
}

.tabs-basic__menu a.active {
  color: #00395d;
  border-bottom: 4px solid #00395d;
}

.link,
.link--external,
.link--internal {
  display: block;
  font-family: Expert-Sans-Light,Helvetica,Helvetica,Arial,sans-serif;
  font-size: 1em;
  line-height: 1.15;
  margin-bottom: .9375em;
}

.link,
.link--external,
.link--internal {
  line-height: 1.125;
  font-size: 1.125em;
}

.link--internal:after {
  top: 2px;
  position: relative;
}

@media print {
  a[href]:after {
    content: "";
  }

  .logo-banner__hamburger,
  .logo-banner__search {
    display: none!important;
  }

  .logo-banner__logo {
    text-align: left;
    margin-bottom: 1em;
  }

  .logo-banner {
    border-bottom: 0;
  }

  .jump-to-nav,
  .latest-news,
  .link__page_holder,
  .related-content,
  .share-tools,
  .share-tools-2,
  .topics,
  .topics-2 {
    display: none!important;
  }

  blockquote {
    border: 0;
  }

  .bottom-footer__links,
  .carousel__controls-wrapper {
    display: none;
  }
}