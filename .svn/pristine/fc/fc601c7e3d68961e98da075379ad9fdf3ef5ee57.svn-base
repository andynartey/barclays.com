<!-- Logo banner -->
<section class="logo-banner">
    <div class="row">
        <div class="logo-banner__hamburger">
            <a href="" class="js-hamburger-toggle">
                <img src="/images/icon-hamburger.png" alt="">
            </a>
        </div>
        <div class="logo-banner__logo">
            <a href="">
                <img data-interchange="
                    [/images/logo-tab-mob.png, (default)],
                    [/images/logo-tab-mob.png, (medium)],
                    [/images/logo.png, (large)]" alt="Barclays">
            </a>
        </div>
        <div class="logo-banner__search">
            <a href="" class="logo-banner__search__toggle">Search</a>
            <form action="" class="logo-banner__search__form">
                <input type="submit" value="Go" class="logo-banner__search-btn medium-1">
                <input type="text" placeholder="Search" class="logo-banner__search-field">
            </form>
        </div>
    </div>
</section>